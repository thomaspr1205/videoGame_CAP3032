  Level(int levelNum) {
    if(levelNum == 1) {
      map = lvl1;
    }
    else if(levelNum == 2) {
      map = lvl2;
    }
    else if(levelNum == 3) {
      map = lvl3;
    }
    else if(levelNum == 4) {
      map = lvl4;
    }
  }
  
  
  void loadLevel() {
    background(#84DFF7);
    for(int i = 0; i < 28; i++) { // row
      for(int j = 0; j < 40; j++) { // columns
        if(map[i][j] == 1) {
          img = loadImage("data/Textures-16.png");
          img = img.get(0,0,16,16);
          image(img, j*25, i*25, 25, 25);
        }
      }
    }
    
  }
  
